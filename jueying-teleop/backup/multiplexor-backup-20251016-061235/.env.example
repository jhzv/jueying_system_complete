# Multiplexor Configuration
PORT=8080
NODE_ENV=production

# ROS Bridge
ROSBRIDGE_URL=ws://localhost:9090

# Robot Network
ROBOT_MOTION_HOST=192.168.1.120
ROBOT_PERCEPTION_HOST=192.168.1.103

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRATION=24h

# Rate Limiting
MAX_COMMAND_RATE=20
TELEMETRY_RATE=10

# Logging
LOG_LEVEL=info
LOG_FILE=/tmp/multiplexor.log
